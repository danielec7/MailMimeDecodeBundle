<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
				<parameter key="ijanki_mail_mime_decode.class" />
        <parameter key="ijanki_mail_mime_decode.decode_bodies" />
        <parameter key="ijanki_mail_mime_decode.include_bodies" />
				<parameter key="ijanki_mail_mime_decode.rfc822_bodies" />
				<parameter key="ijanki_mail_mime_decode.decode_headers" />
    </parameters>

    <services>
        <service id="ijanki_mail_mime_decode" class="%ijanki_mail_mime_decode.class%">
            <argument>%ijanki_mail_mime_decode.decode_bodies%</argument>
						<argument>%ijanki_mail_mime_decode.include_bodies%</argument>
						<argument>%ijanki_mail_mime_decode.rfc822_bodies%</argument>
						<argument>%ijanki_mail_mime_decode.decode_headers%</argument>
        </service>
    </services>
</container>
